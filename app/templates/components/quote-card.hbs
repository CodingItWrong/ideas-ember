<PaperCard as |Card|>
  <Card.title as |Title|>
    <Title.text as |Text|>
      <Text.headline>{{@quote.text}}</Text.headline>
      <Text.subhead>{{yield}}</Text.subhead>
    </Title.text>
  </Card.title>
  <Card.actions>
    <PaperButton @warn={{true}} @onClick={{this.delete}}>
      Delete
    </PaperButton>
  </Card.actions>
</PaperCard>
