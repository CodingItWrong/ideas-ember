<PaperCard as |Card|>
  <Card.title as |Title|>
    <Title.text as |Text|>
      <Text.headline>{{@source.title}}</Text.headline>
      <Text.subhead>{{@source.author.name}}</Text.subhead>
    </Title.text>
  </Card.title>
  <Card.actions>
    <PaperButton @onClick={{this.goToSourceDetail}}>
      Details
    </PaperButton>
  </Card.actions>
</PaperCard>
